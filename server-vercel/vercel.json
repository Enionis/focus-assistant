{
  "version": 2,
  "functions": {
    "api/*.ts": {
      "runtime": "nodejs18.x"
    }
  },
  "routes": [
    {
      "src": "/api/webhook",
      "dest": "/api/webhook.ts"
    },
    {
      "src": "/api/sync",
      "dest": "/api/sync.ts"
    },
    {
      "src": "/api/cron",
      "dest": "/api/cron.ts"
    }
  ]
}